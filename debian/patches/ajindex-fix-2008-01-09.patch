--- a/ajax/ajindex.c
+++ b/ajax/ajindex.c
@@ -2516,7 +2516,14 @@
 
 static ajint btreeNumIdCompare(const void *a, const void *b)
 {
-    return (*(AjPBtNumId const *)a)->offset - (*(AjPBtNumId const *)b)->offset;
+  ajlong val;
+
